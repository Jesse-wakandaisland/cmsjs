{
  "name": "cmsjs",
  "version": "1.0.0",
  "description": "CMS.js - Integrated PocketBase + Electric + Derby.js platform with infinite generative capabilities",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:pocketbase\"",
    "dev:backend": "node src/server/index.js",
    "dev:frontend": "vite",
    "dev:pocketbase": "node src/pocketbase/wrapper.js",
    "build": "vite build",
    "build:mobile": "npm run build && cap sync",
    "preview": "vite preview",
    "serve": "node src/server/index.js",
    "install:pocketbase": "node scripts/install-pocketbase.js",
    "postinstall": "npm run install:pocketbase"
  },
  "keywords": [
    "cms",
    "pocketbase",
    "electric-sql",
    "derby",
    "pglite",
    "infinite-generation",
    "aframe",
    "3d"
  ],
  "author": "Jesse-wakandaisland",
  "license": "Apache-2.0",
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1",
    "@electric-sql/pglite": "^0.1.5",
    "aframe": "^1.5.0",
    "aframe-extras": "^7.0.0",
    "aframe-environment-component": "^1.3.3",
    "derby": "^4.0.0-beta.12",
    "racer": "^1.1.3",
    "sharedb": "^3.0.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "concurrently": "^8.2.2",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4"
  }
}
